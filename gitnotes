ALTER TABLE TN ADD(C1 DT(), C2 DT()); -- ADDING COL
ALTER TABLE TN DROP COL C1; -- DROP COL
ALTER TABLE TN MODIFY COL COL_TP

SELECT 
FROM E.NAME, E.SAL, D.DNAME
EMP E, DEPT D
WHERE E.DEPTNO =D.DEPTNO;

SELECT 
FROM E.NAME, E.SAL, D.DNAME
EMP E INNER JOIN DEPT D
ON E.DEPTNO =D.DEPTNO;
